def intliteral : Int -> Int
	| 0 = 10
	| n = -10

def floatliteral : Int -> FloatPattern
	| 0 = 0.1
	| n = -0.1

def charliteral : Int -> Char
	| 0 = 'a'
	| 1 = '\n'
	| 2 = '0'
	| 3 = '+'
	| 4 = '\''
	| 5  = '"'
	| 6  = '\\'

def stringlitetral : Int -> String
	| 0 = ""
	| 1 = "1"
	| 2 = "111"
	| 3 = "a b"
	| 4 = "\""
	| 5 = "\\"
	| 6 = "\n"
	| n = "Hello World!"

def booleanliteral : Int -> Bool
	| 0 = True
	| n = False
